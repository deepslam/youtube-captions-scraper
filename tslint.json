{
  "rules": {
    "no-console": [false],
    "variable-name": false,
    "radix": false,
    "object-literal-sort-keys": false,
    "trailing-comma": [false],
    "indent": [false, "tabs"],
    "max-line-length": [false],
    "no-string-literal": false,
    "class-name": false,
    "no-namespace": [false],
    "no-bitwise": false,
    "no-var-requires": false,
    "arrow-parens": false
  },
  "extends": "tslint:recommended",
  "linterOptions": {
    "exclude": ["*.json", "**/*.json"]
  }
}
